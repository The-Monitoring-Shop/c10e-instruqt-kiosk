{
  "manifest_version": 3,
  "name": "Chronosphere - Auto Login",
  "version": "1.0.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "c10e-autoLogin@chronosphere.io"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "autoLogin.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "key": ""
}